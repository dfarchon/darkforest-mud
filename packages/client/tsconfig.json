{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": ["vite/client"],
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@df/*": ["./src/Shared/*"]
    },
    "strictPropertyInitialization": false,
    "useDefineForClassFields": false
  },
  "include": ["src"]
}
