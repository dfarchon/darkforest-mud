{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "vite/client"
    ],
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "useDefineForClassFields": false,
    "jsx": "react-jsx",
    "paths": {
      "@df/*": [
        "./src/Shared/*"
      ],
      "@mud/*": [
        "./src/mud/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@wallet/*": [
        "./src/wallet/*"
      ],
      "@frontend/*": [
        "./src/Frontend/*"
      ],
      "@backend/*": [
        "./src/Backend/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts"
  ]
}